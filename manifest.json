{
  "manifest_version": 3,
  "name": "SemanticSearch",
  "description": "search website including synonyms and semantic similiar sentence",
  "version": "1.0.0",
  "author": "silent way",
  "action": {
    "default_icon": "assets/house.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "scripting",
